{
  "private": true,
  "scripts": {
    "all": "cross-env NODE_ENV=development PLATFORM=web SCRIPT=frontend_backend webpack-dev-server --inline --hot --watch",
    "start": "cross-env NODE_ENV=development PLATFORM=web webpack-dev-server --config webpack/webpack.config.js --inline --hot --watch --host=0.0.0.0  ",
    "backend": "cross-env NODE_ENV=development SCRIPT=backend PLATFORM=web webpack-dev-server --inline --hot --watch",
    "dashboard": "webpack-dashboard --webpack-dev-server --inline --hot --watch",
    "build_frontend": "cross-env NODE_ENV=production SCRIPT=frontend PLATFORM=web webpack --config webpack/webpack.production.config.js --progress --profile --colors",
    "build_backend": "cross-env NODE_ENV=production SCRIPT=backend PLATFORM=web webpack --config webpack/webpack.production.config.js --progress --profile --colors",
    "build": "cross-env NODE_ENV=production SCRIPT=frontend_backend PLATFORM=web webpack --config webpack/webpack.production.config.js --progress --profile --colors; npm run build --prefix resources/web3-node ",
    "dev": "lite-server",
    "dev:ganache": "ganache-cli",
    "compile": "truffle compile --all",
    "dev:test": "truffle compile --all && mocha --require test/testPolyfill.js --require babel-polyfill --require babel-register --recursive test/unitTests/",
    "deploy": "mocha --require test/testPolyfill.js --require babel-polyfill --require babel-register scripts/deploy.js"
  },
